{
  "name": "billybunn",
  "private": true,
  "description": "A portfolio site for Billy Bunn, a software developer in Seattle, WA.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "deploy": "gatsby build && gh-pages -d public -b master",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "core-js"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@mdx-js/mdx": "^1.5.3",
    "@mdx-js/react": "^1.5.3",
    "@reach/dialog": "^0.2.0",
    "@theme-ui/components": "^0.2.50",
    "@theme-ui/presets": "^0.2.44",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.18.17",
    "gatsby-image": "^2.0.39",
    "gatsby-plugin-mdx": "^1.0.67",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-sitemap": "^2.0.12",
    "gatsby-plugin-theme-ui": "^0.2.53",
    "gatsby-remark-copy-linked-files": "^2.1.5",
    "gatsby-remark-images": "^3.1.8",
    "gatsby-remark-prismjs": "^3.3.5",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.0.30",
    "gatsby-transformer-remark": "^2.3.9",
    "gatsby-transformer-sharp": "^2.1.18",
    "gh-pages": "^2.1.1",
    "intersection-observer": "^0.5.1",
    "lodash.merge": "^4.6.2",
    "polished": "^3.4.2",
    "prismjs": "^1.18.0",
    "prop-types": "^15.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-spring": "^9.0.0-beta.34",
    "resize-observer-polyfill": "^1.5.1",
    "theme-ui": "^0.2.52",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "chrome-launcher": "^0.10.5",
    "core-js": "^2.6.2",
    "directory-named-webpack-plugin": "^4.0.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "favicons": "^5.3.0",
    "gatsby-plugin-eslint": "^2.0.3",
    "lighthouse": "^4.0.0",
    "mkdirp": "^0.5.1",
    "node-gyp": "^3.8.0",
    "prettier": "^1.16.0"
  }
}
